{
  "name": "react-boilerplate-client-only",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run clean && cross-env DEBUG=app:* NODE_ENV=development babel-watch server/index.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --progress --config webpack.config.js",
    "clean": "rimraf build",
    "precommit": "git diff --cached --name-only | grep -e '^\\(src\\|server\\).*\\.js$' | xargs eslint",
    "lint": "eslint src server",
    "lint:fix": "eslint --fix src server",
    "format": "prettier --single-quote --semi=false --write '{src,server}/**/*.js' && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:madurmanov/react-boilerplate-client-only.git"
  },
  "author": "Mikhail Durmanov",
  "license": "MIT",
  "dependencies": {
    "fetch-everywhere": "^1.0.5",
    "form-data": "^2.3.2",
    "history": "^4.7.2",
    "lodash": "^4.17.5",
    "prop-types": "^15.6.0",
    "query-string": "^5.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-jss": "^8.4.0",
    "react-redux": "^5.0.6",
    "react-universal-component": "^2.8.2",
    "redux": "^3.7.2",
    "redux-first-router": "^1.9.19",
    "redux-first-router-link": "^1.4.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-universal-import": "^2.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-watch": "^2.0.6",
    "body-parser": "^1.18.2",
    "cross-env": "^5.1.3",
    "debug": "^3.1.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "express": "^4.16.2",
    "husky": "^0.14.3",
    "prettier": "^1.4.4",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "2.21.0"
  }
}
