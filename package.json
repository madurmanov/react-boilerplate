{
  "name": "react-config",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "precommit": "git diff --cached --name-only | grep -e '\\.js$' | xargs eslint",
    "start": "DEBUG=src:* node src/server/index.js",
    "start-prod": "NODE_ENV=production npm run start",
    "lint": "eslint src",
    "build": "rm -rf dist && webpack --config webpack.config.js"
  },
  "author": "Mikhail Durmanov",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^4.15.0",
    "eslint-config-madurmanov": "git+ssh://git@github.com/madurmanov/eslint-config.git",
    "express": "^4.16.2",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-exponentiation-operator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "debug": "^3.1.0",
    "file-loader": "^1.1.6",
    "history": "^4.7.2",
    "jss": "^9.5.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-jss": "^8.2.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "webpack": "^3.10.0"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-exponentiation-operator"
    ]
  },
  "eslintConfig": {
    "extends": "madurmanov"
  }
}
